
{
  "manifest_version": 3,
  "name": "One-click Incognito",
  "version": "1.0",
  "permissions": ["scripting", "tabs"],
  "action": {
    "default_icon": {
      "48": "incognito-128_x_128.png"
    }
  },

  "icons": {
    "16": "incognito-16_x_16.png",
    "48": "incognito-48_x_48.png",
    "128": "incognito-128_x_128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
